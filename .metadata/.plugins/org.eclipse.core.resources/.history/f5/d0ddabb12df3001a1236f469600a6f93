package Teste;

public class ArrayTest {

	public static void main(String[] args) {
		
		String[] array1 = new String[10];
		String[] array2 = new String[10];
		String[] concat = new String [10];

		array1[0] = "agua";
		array1[1] = "terra";
		array1[2] = "sol";
		array1[3] = "lua";
		array1[4] = "bola";
		array1[5] = "chuva";

		array2[0] = "agua";
		array2[1] = "marte";
		array2[2] = "sol";
		array2[3] = "chuva";
		array2[4] = "terremoto";
		array2[5] = "CHUVA";
		array2[6] = "bola";
		
		for(int i = 0; i < array1.length; i++) {
			for(int j = 0; j < array2.length; j++) {
				if(array1[0].toLowerCase().equals(array2[0].toLowerCase())) {
					concat[i] = array1[i];
				}
			}
		}

	}
}
