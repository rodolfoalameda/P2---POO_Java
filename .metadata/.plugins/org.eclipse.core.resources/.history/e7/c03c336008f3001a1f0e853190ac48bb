package Ex2;

import java.util.TreeSet;

public class HashSetXTreeSet {

	public static void main(String[] args) {

		long start = System.currentTimeMillis();

		TreeSet<Integer> example = new TreeSet<Integer>();
		int total = 300000;
		
		for (int i = 0; i <= total; i++) {
			example.add(i);
		}
		
		long elapsedTime = System.currentTimeMillis() - start;
		System.out.println("Tempo gasto Hs: " + elapsedTime);
		
///////////////////////////////////////////////////////////////////////////////
		
		for (int i = 0; i < example.size(); i++) {
			example.contains(i);
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		long start3 = System.currentTimeMillis();
		
		for (int i = 0; i < example.size(); i++) {
			example.remove(i);
		}
		
		long elapsedTime3 = System.currentTimeMillis() - start3;
		
		System.out.println("Tempo gasto Hs: " + elapsedTime3);
	}

}
