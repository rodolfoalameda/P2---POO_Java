package Teste;

public class ArrayTest {

	String[] array1 = new String[10];
	String[] array2 = new String[10];
	
	public static void main(String[] args) {
		
		ArrayTest example = new ArrayTest();
		
		example.array1[0] = "agua";
		example.array1[1] = "terra";
		example.array1[2] = "sol";
		example.array1[3] = "lua";
		example.array1[4] = "bola";
		example.array1[5] = "chuva";
		
		example.array2[0] = "agua";
		example.array2[0] = "marte";
		example.array2[0] = "sol";
		example.array2[0] = "chuva";
		example.array2[0] = "terremoto";
		example.array2[0] = "CHUVA";
		example.array2[0] = "bola";
	
		for(int i = 0; i < example.array1.length; i++) {
			System.out.println(example.array1[i]);
		}
		
	}	
}
